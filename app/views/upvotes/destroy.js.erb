function upvote(innerHTML) {
  const upvote = document.getElementById('upvote-btn-creator-<%= @creator.id %>')
  upvote.innerHTML = innerHTML;

}

if (document.getElementById("show-creator")) {
  upvote("<%= j render 'shared/upvote_full_btn', creator: @creator %>");
} else {
  // document.getElementById("homepage")
  upvote("<%= j render 'shared/upvote-button', creator: @creator %>");
}

